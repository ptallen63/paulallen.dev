  "devDependencies": {
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-eslint": "^2.0.1",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "netlify-lambda": "^1.4.2",
    "prettier": "^1.16.4"
  },
  "license": "MIT",
  "scripts": {
    "build": "netlify-lambda build ./src/netlify-functions && gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "serve:lambda": "netlify-lambda serve ./src/netlify-functions",
    "build:lambda": "netlify-lambda build ./src/netlify-functions",
    "lint": "eslint src",
    "predeploy": "git checkout master && git pull",
    "deploy": "git checkout production && git pull && git merge master && git push && git checkout master",
    "postdeploy": "echo \"Deployment complete!\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  }
